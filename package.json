{
  "name": "@brillout/json-s",
  "version": "0.4.4",
  "description": "Same as JSON but with added support for `Date`, `undefined`, `Map`, `Set`, and more.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "node": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./parse": {
      "node": "./dist/cjs/parse.js",
      "import": "./dist/esm/parse.js"
    },
    "./stringify": {
      "node": "./dist/cjs/stringify.js",
      "import": "./dist/esm/stringify.js"
    }
  },
  "scripts": {
    "dev": "npm run tsc:watch:cjs",
    "build": "npm run clean && npm run tsc:esm && npm run tsc:cjs",
    "test": "self-import && node test/",
    "docs": "mdocs",
    "tsc:esm": "tsc",
    "tsc:cjs": "tsc --project ./tsconfig.cjs.json",
    "tsc:watch:esm": "tsc --incremental --watch",
    "tsc:watch:cjs": "tsc --incremental --watch --project ./tsconfig.cjs.json",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@brillout/mdocs": "^0.1.30",
    "@types/node": "17.0.13",
    "lodash.isequal": "^4.5.0",
    "react": "^17.0.2",
    "self-import": "^1.0.1",
    "typescript": "^4.5.5"
  },
  "files": [
    "dist/",
    "*.d.ts",
    "*.js"
  ],
  "repository": "github:brillout/json-s",
  "publishConfig": {
    "access": "public"
  }
}
